{
  "preview": {
    "services": {
      "nextjs": {
        "type": "bun",
        "directory": ".",
        "is_entrypoint": true
      },
      "engine": {
        "type": "python",
        "directory": "./engine",
        "connection_variable": "ENGINE_URL"
      }
    },
    "storages": {
      "postgres": {
        "type": "postgres",
        "connection_variable": "POSTGRES_URL"
      },
      "redis": {
        "type": "redis",
        "connection_variable": "REDIS_URL"
      }
    },
    "environment": {
      "BETTER_AUTH_SECRET": "mysecret",
      "NEXT_PUBLIC_APP_URL": "http://localhost:3000",
      "RESEND_API_KEY": "__KSK__RESEND_API_KEY__",
      "RESEND_FROM_EMAIL": "templates@kosuke.ai",
      "RESEND_FROM_NAME": "Kosuke Template",
      "RESEND_REPLY_TO": "templates@kosuke.ai"
    }
  }
}
